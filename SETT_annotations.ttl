sett-annotations:Browning a skos:Concept ;
    skos:broader sett-annotations:Vegetation ;
    skos:definition "Indicates decreasing slope direction in vegetation, such as decline or degradation." ;
    skos:notation "Browning" ;
    skos:prefLabel "Browning" .

